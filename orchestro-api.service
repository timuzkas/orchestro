[Unit]
Description=Orchestro API Server
After=network.target docker.service
Requires=docker.service

[Service]
Type=simple
User=debian
WorkingDirectory=/home/debian/orchestro/api
ExecStart=/home/debian/orchestro/api/main
Restart=always
RestartSec=5
Environment=GIN_MODE=release

[Install]
WantedBy=multi-user.target
